<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Wiki Tests" parallel="false">
    <test name="Navigate to Main Page">
        <classes>
            <class name="selenium.pom.NavigateToMainPageTest"/>
        </classes>
    </test>
    <test name="Search From Home Page">
        <classes>
            <class name="selenium.pom.SearchFromHomePage"/>
        </classes>
    </test>
    <test name="Auth tests">
        <classes>
            <class name="selenium.pom.LoginTest"/>
        </classes>
    </test>
    <test name="Alert tests">
        <classes>
            <class name="selenium.alerts.AlertTest"/>
        </classes>
    </test>
</suite>