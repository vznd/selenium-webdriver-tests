<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Wiki Tests" parallel="false">
    <test name="Navigate to Main Page">
        <classes>
            <class name="wiki.navigation.NavigateToMainPageTest"/>
        </classes>
    </test>
    <test name="Search From Home Page">
        <classes>
            <class name="wiki.search.SearchFromHomePage"/>
        </classes>
    </test>
    <test name="Auth tests">
        <classes>
            <class name="wiki.auth.LoginTest"/>
        </classes>
    </test>
    <test name="Alert tests" enabled="false">
        <classes>
            <class name="wiki.alerts.AlertTest"/>
        </classes>
    </test>
</suite>